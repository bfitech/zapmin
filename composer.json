{
  "name": "bfitech/zapmin",
  "description": "A simple authentication manager.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rendra Suroso",
      "email": "rs@cogsci.bandungfe.net"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5 || ~7.0",
    "bfitech/zapstore-sqlite3": "dev-master as 1.2.x-dev"
  },
  "require-dev": {
    "bfitech/zapstore-mysql": "dev-master as 1.2.x-dev",
    "bfitech/zapstore-pgsql": "dev-master as 1.2.x-dev",
    "phpunit/phpunit": "4.8.*"
  },
  "suggest": {
    "bfitech/zapstore-mysql": "Use MySQL as database backend.",
    "bfitech/zapstore-pgsql": "Use PostgreSQL as database backend."
  },
  "autoload": {
    "psr-4": {
      "BFITech\\ZapAdmin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BFITech\\ZapCoreDev\\": ""
    }
  }
}
